---
import IconNetwork from "./IconNetwork.astro";

export interface Props {
  socialMedia: Array<{
    nameIcon: string;
    href: string;
  }>;
}

const { socialMedia } = Astro.props;
---

<div class="flex flex-row gap-x-2">
  {socialMedia.map((social) => (
    <IconNetwork nameIcon={social.nameIcon} href={social.href} />
  ))}
</div>
